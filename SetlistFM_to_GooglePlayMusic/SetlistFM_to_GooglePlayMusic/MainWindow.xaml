<Window x:Class="SetlistFM_to_GooglePlayMusic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:SetlistFM_to_GooglePlayMusic"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow"
        Width="525"
        Height="500"
        mc:Ignorable="d">
    <StackPanel>
        <TextBox x:Name="tbox_ArtistName" />
        <TextBlock x:Name="tblock_Artists" Height="300" />
        <Button x:Name="button_SearchArtists"
                Width="100"
                Height="25"
                Click="button_SearchArtists_Click" />
    </StackPanel>

</Window>
